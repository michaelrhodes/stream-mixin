{
  "name": "stream-mixin",
  "version": "0.0.0",
  "description": "A classic-stream mixin for browser code.",
  "main": "index.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "dependencies": {
    "emitter-component": "1.0.1"
  },
  "devDependencies": {
    "tape": "~2.1.0"
  },
  "scripts": {
    "test": "tape test/*.js"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": {
      "ie": [6, 7, 8, 9, 10],
