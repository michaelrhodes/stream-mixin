      "safari": [5.1, 6],
      "opera": [10, 12, 15],
      "iphone": [6],
      "android": [4.2]
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:michaelrhodes/stream-mixin.git"
  },
  "keywords": [
    "classic",
    "stream",
    "browser",
    "crollian",
    "mixin"
  ],
